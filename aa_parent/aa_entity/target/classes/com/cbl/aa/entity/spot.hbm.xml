<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cbl.aa.entity.Spot" table="spot" >
        <id name="id" >
            <generator class="native"/>
        </id>

        <property name="province" /> 
        <property name="province_id"/> 
        <property name="b_city" /> 
        <property name="b_city_id" /> 
        <property name="city" /> 
        <property name="city_id" /> 
        <property name="city_img" /> 
        <property name="spot" /> 
        <property name="spot_id" /> 
        <property name="spot_url" /> 
        <property name="spot_ct" /> 
        <property name="spot_time" /> 
        <property name="spot_gd" /> 
        <property name="spot_bd" /> 
    </class>
</hibernate-mapping>
